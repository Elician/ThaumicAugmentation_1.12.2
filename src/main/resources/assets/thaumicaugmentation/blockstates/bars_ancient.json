{
    "multipart": [
        {
            "apply": {
                "model": "thaumicaugmentation:bars_ancient_post_ends"
            }
        },
        {   
            "when": { 
                "north": false,
                "east": false,
                "south": false,
                "west": false 
            },
            "apply": {
                "model": "thaumicaugmentation:bars_ancient_post"
            }
        },
        {   
            "when": {
                "north": true,
                "east": false,
                "south": false,
                "west": false 
            },
            "apply": {
                "model": "thaumicaugmentation:bars_ancient_cap",
                "uvlock": true
            }
        },
        {   
            "when": { 
                "north": false,
                "east": true,
                "south": false,
                "west": false
            },
            "apply": {
                "model": "thaumicaugmentation:bars_ancient_cap",
                "y": 90
            }
        },
        {   
            "when": {
                "north": false,
                "east": false,
                "south": true,
                "west": false
            },
            "apply": {
                "model": "thaumicaugmentation:bars_ancient_cap",
                "y": 180
            }
        },
        {   
            "when": {
                "north": false,
                "east": false,
                "south": false,
                "west": true
            },
            "apply": {
                "model": "thaumicaugmentation:bars_ancient_cap",
                "y": 270
            }
        },
        {   
            "when": {
                "north": true
            },
            "apply": {
                "model": "thaumicaugmentation:bars_ancient_side"
            }
        },
        {   
            "when": {
                "east": true
            },
            "apply": {
                "model": "thaumicaugmentation:bars_ancient_side",
                "y": 90
            }
        },
        {   
            "when": {
                "south": true
            },
            "apply": {
                "model": "thaumicaugmentation:bars_ancient_side",
                "y": 180
            }
        },
        {   
            "when": {
                "west": true
            },
            "apply": {
                "model": "thaumicaugmentation:bars_ancient_side",
                "y": 270
            }
        }
    ]
}

