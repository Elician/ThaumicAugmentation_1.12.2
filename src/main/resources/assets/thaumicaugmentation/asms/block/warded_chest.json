{
    "parameters": {
        "end_anim": ["compose", ["+", "#open_speed"], "#open_time"],
        "trigger_end": ["-", "#end_anim"],
        "anim_progress": ["-/-", "#open_time", "#open_speed", 0.01]
    },
    "clips": {
        "model": "thaumicaugmentation:block/warded_chest_anim@model",
        "open": ["apply", "#model", 0.5],
        "opening": [
            "trigger_positive",
            ["apply", "thaumicaugmentation:block/warded_chest_anim@opening", "#anim_progress"],
            "#trigger_end",
            "!transition:open"
        ],
        "closing": [
            "trigger_positive",
            ["apply", "thaumicaugmentation:block/warded_chest_anim@closing", "#anim_progress"],
            "#trigger_end",
            "!transition:closed"
        ],
        "closed": ["apply", "#model", 0]
    },
    "states": [
        "open",
        "opening",
        "closing",
        "closed"
    ],
    "transitions": {
        "opening": ["open", "closing", "closed"],
        "open": ["closing", "closed", "opening"],
        "closing": ["closed", "opening", "closing"],
        "closed": ["opening", "open", "closing"]
    },
    "start_state": "closed"
}